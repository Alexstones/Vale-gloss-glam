datasource db {
  provider = "sqlite"
}

generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
}

model Booking {
  id        String   @id @default(cuid())
  name      String
  phone     String
  service   String
  notes     String?
  startAt   DateTime
  endAt     DateTime
  createdAt DateTime @default(now())

  @@index([startAt])
  @@index([endAt])
}
